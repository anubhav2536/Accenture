 public class HelloWorld{

     public static void main(String []args){
         int[] a={ 12, 3, 14, 56, 77, 15 };

        System.out.println(Don(a,6,2,13));
     }
     public static int Don(int[] arr,int n,int diff,int num){
          int count = 0;
        for (int i = 0; i < n; i++)
        {
            if (Math.abs (num - arr[i]) <= diff)
                count++;
        }
        return count;
    }
     }
